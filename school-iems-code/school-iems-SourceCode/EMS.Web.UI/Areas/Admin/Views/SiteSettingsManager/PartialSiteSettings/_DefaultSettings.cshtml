
<div class="row">
    <!--Col start-->
    <div class="col-md-6 table-form">
        <table class="form" width="100%" border="0" cellspacing="2" cellpadding="3">
            <tbody>
                <tr>
                    <td class="fieldlabel">Institute Logo Color Solo </td>
                    <td class="fieldarea">
                        <input type="text" ng-model="SiteSettings.InstituteLogoColorSolo" class="form-control" placeholder="Enter Institute Logo Color Solo" required="" />
                    </td>
                    <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Institute Logo Color Solo." data-toggle="tooltip"></i></span></td>
                </tr>

                <tr>
                    <td class="fieldlabel">Institute Logo Color Short </td>
