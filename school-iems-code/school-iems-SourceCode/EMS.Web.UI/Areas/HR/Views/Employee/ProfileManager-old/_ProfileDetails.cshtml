                          <i class="fa fa-credit-card circular"></i>
                      </span>
                  </div>*@
                  <div class="col-sm-3 form-group">
                      <label class="control-label">Employee Category <span  class="red glyphicon glyphicon-asterisk"></span></label>
                      <select ng-model="Obj.Employee.@User_Employee.Property_EmployeeCategoryEnumId" ng-options="option.Id as option.Name for option in EmployeeCategoryEnumList " ng-init="" class="form-control"></select>
                  </div>
                  <div class="col-sm-3 form-group">
                      <label class="control-label">Employee Type <span  class="red glyphicon glyphicon-asterisk"></span></label>
                      <select ng-model="Obj.Employee.@User_Employee.Property_EmployeeTypeEnumId" ng-options="option.Id as option.Name for option in EmployeeTypeEnumList " ng-init="" class="form-control"></select>
                  </div>
                  <div class="col-sm-3 form-group">
                      <label class="control-label">Job Class <span  class="red glyphicon glyphicon-asterisk"></span></label>
                      <select ng-model="Obj.Employee.@User_Employee.Property_JobClassEnumId" ng-options="option.Id as option.Name for option in JobClassEnumList " ng-init="" class="form-control"></select>
                  </div>
                  <div class="col-sm-3 form-group">
                      <label class="control-label">Job Type <span  class="red glyphicon glyphicon-asterisk"></span></label>
                      <select ng-model="Obj.Employee.@User_Employee.Property_JobTypeEnumId" ng-options="option.Id as option.Name for option in JobTypeEnumList " ng-init="" class="form-control"></select>
                  </div>
                  <div class="col-sm-3 form-group">
                      <label class="control-label">Campus <span  class="red glyphicon glyphicon-asterisk"></span></label>
                      <select ng-model="Obj.Account.@User_Account.Property_CampusId" ng-options="option.Id as option.Name for option in CampusList " ng-init="" class="form-control"></select>
                  </div>
                  <div class="col-sm-3 form-group">
                      <label class="control-label">Working Status <span  class="red glyphicon glyphicon-asterisk"></span></label>
                      <select ng-model="Obj.Employee.@User_Employee.Property_WorkingStatusEnumId" ng-options="option.Id as option.Name for option in WorkingStatusEnumList " ng-init="" class="form-control"></select>
                  </div>
                  <div class="col-sm-3 form-group">
                      <label class="control-label">
                          Joining Date (dd/mm/yyyy) <span  class="red glyphicon glyphicon-asterisk"></span>
                          <button type="button" class="btn-none" data-toggle="tooltip" data-placement="top"
                                  title="Your Official Joining Date in @SiteSettings.Instance.InstituteShortName (dd/mm/yyyy ex:19/04/1998)">
                              <i class="fa fa-question-circle"></i>
                          </button>
                      </label>
                      <span  class="input-icon icon-right">
                          <input ng-change="changeJoiningDate()" ng-model="Obj.Employee.JoiningDateString" class="form-control " type="text" data-mask="99/99/9999" placeholder="dd/mm/yyyy">
                          <i class="glyphicon glyphicon-calendar circular"></i>
                      </span>
                  </div>
                  <div class="col-sm-3 form-group">
                      <label class="control-label">
                          Leaving Date (dd/mm/yyyy) <span  class="red glyphicon glyphicon-asterisk"></span>
                          <button type="button" class="btn-none" data-toggle="tooltip" data-placement="top"
