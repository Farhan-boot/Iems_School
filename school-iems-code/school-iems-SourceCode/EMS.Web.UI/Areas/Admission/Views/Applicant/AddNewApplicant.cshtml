@using EMS.DataAccess.Data
@using EMS.Framework.Settings
@using EMS.Framework.Utils

@{
    ViewBag.Title = "Add New Applicant";
    //Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
    // Angular Properties
    var id = Request.QueryString["id"];//ViewContext.RouteData.Values["id"];
    var AccountId = (string)id == null ? "0" : (string)id;

    //WebApi link for AddNewApplicant
    var getAccountByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "AddNewApplicantApi", action = "GetStudentById", area = "Admission" });
    var saveAccountUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "AddNewApplicantApi", action = "SaveStudent", area = "Admission" });
    var getDataExtraOnChangeProgramUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "AddNewApplicantApi", action = "GetDataExtraOnChangeProgram", area = "Admission" });

    var getMaxAndSuggestUsernameUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "AddNewApplicantApi", action = "GetMaxAndSuggestUsername", area = "Admission" });

    var getPreviousStudentInformationUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "AddNewApplicantApi", action = "GetPreviousStudentInformation", area = "Admission" });

    //============= Start Package & Discount ===========

    var getStdScholarshipDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "AddNewApplicantApi", action = "GetStdScholarshipForApplicantDataExtra", area = "Admission" });

    var saveScholarshipListByStudentUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "StudentProfileApi", action = "SaveScholarshipListByStudent", area = "Admission" });
    var getNewScholarshipByStudentIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "StudentProfileApi", action = "GetNewScholarshipByStudentId", area = "Admission" });
    var getLockOrUnlockScholarshipByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "StudentProfileApi", action = "GetLockOrUnlockScholarshipById", area = "Admission" });

    var getAllScholarshipByStudentIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "StudentProfileApi", action = "GetAllScholarshipByStudentId", area = "Admission" });
    var getTrashScholarshipByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "StudentProfileApi", action = "GetTrashScholarshipById", area = "Admission" });

    //Semester Wise Scholarship ForNewApplicantDataExtra
    var getSemWiseScholarshipDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "AddNewApplicantApi", action = "GetSemWiseScholarshipForNewApplicantDataExtra", area = "Admission" });


    // Angular Functions
    string getAccountById = "getAccountById";
    var saveAccount = "saveAccount";
    var getNewAccount = "getNewAccount";
    var refreshPage = "loadPage()";

    string onChangeProgram = "onChangeProgram";
    string onChangeExam = "onChangeAdmissionExam";
    string onChangeFeeCode = "onChangeFeeCode";


}
@section ScriptSection
{
    <script src="~/Areas/Admission/Views/Applicant/AddNewApplicant.js?v=8.5"></script>
    

}
@section StyleSection
{
    @* Todo Need to add BundleConfig *@
    <link href="~/assets/js/angular/angular-ui-select/select2.css" rel="stylesheet" />
}
<div id="AddNewApplicant" class="row" ng-controller="AddNewApplicantCtrl" ng-init="Init(
        '@AccountId'
        ,'@getAccountByIdUrl'
        ,'@saveAccountUrl'
        ,'@getDataExtraOnChangeProgramUrl'
        ,'@getStdScholarshipDataExtraUrl'
        ,'@getMaxAndSuggestUsernameUrl'
        ,'@getSemWiseScholarshipDataExtraUrl'
        ,'@getPreviousStudentInformationUrl'
     )">
    <div class="col-lg-12 col-sm-12 col-xs-12">
        <div ng-Hide="!HasError ||ErrorMsg==''" class="alert alert-danger fade in radius-bordered alert-shadowed">
            <button ng-click="HasError=false" class="close" data-dismiss1="alert">Ã—</button>
            <i class="fa-fw fa fa-warning"></i>
            <strong>Attention Please!</strong> {{ErrorMsg}}
        </div>
        @*ng-show="HasViewPermission"*@
        <div>
            <div class="widget">
                <div class="widget-header bordered-bottom bordered-sky ">
                    <i class="widget-icon fa fa-edit bold  "></i>
                    <span class="widget-caption ">
                        Add/Edit
                        @*<strong ng-show="Account.@User_Account.Property_Name!=''">{{(Account.@User_Account.Property_Name)}}</strong>*@
                    </span>
                    <div class="widget-buttons buttons-bordered no-padding-right" style="">
                        <span class="input-icon">
                            <input type="text" ng-model="AccountId" ng-Enter="@(getAccountById)()" class="form-control input-xs " id="glyphicon-search" placeholder="Search by ID">
                            <i class="glyphicon glyphicon-search blue"></i>
                        </span>
                    </div>
                    <div class="widget-buttons no-padding-left">
                        <button ng-click="@(getNewAccount)(0)" class="btn btn-success btn-xs"><i class="fa fa-plus"></i> Add New</button>
                        <a href="@Url.Action("StudentList", "Student", new {area = "Admission"})" class="btn btn-blue btn-xs" target="_blank"><i class="fa fa-arrow-left"></i> Student List</a>
                        <button ng-click="@(refreshPage)" class="btn btn-info btn-xs"><i class="fa fa-refresh"></i> Refresh</button>
                    </div>
                </div><!--Widget Header end-->
                <div class="widget-body">
                    <div class="row">
                        <div class="col-md-5 table-form">
                            <table class="form" width="100%" border="0" cellspacing="2" cellpadding="3">
                                <tbody>

                                    <tr>
                                        <td class="fieldlabel">Admission Session</td>
                                        <td class="fieldarea">
                                            <select ng-model="SelectedAdmissionExam"
                                                    ng-change="@(onChangeExam)()"
                                                    ng-options="option.Name
                                                    @(SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerate ?
                                                        "+' (Prefix:'+option.StudentIdPrefix+')'"
                                                        : SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerateAsUGC ?
                                                            "+' (UGC Prefix:'+option.UgcIdPrefix+')'":"")
                                                    for option in  AdmissionExamList " ng-init="" class="form-control select-inline">
                                                <option value="" label="" class="">---Select One---</option>
                                            </select>
                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Admission Circular" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                    </tr>


                                    <tr>
                                        <td class="fieldlabel">Program</td>
                                        <td class="fieldarea">
                                            <select ng-model="SelectedProgram"
                                                    ng-change="@(onChangeProgram)()"
                                                    ng-options="option.Name
                                                    @(SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerate ?
                                                        "+' (No:'+option.DepartmentCode+')'" :
                                                        SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerateAsUGC ?
                                                            "+' (UGC Code:'+option.UgcCode+')'":"")
                                                    for option in  ProgramList | filter:{SemesterDurationEnumId : SelectedAdmissionExam.SemesterDurationEnumId}" ng-init="" class="form-control select-inline">
                                                <option value="" label="" class="">---Select Program---</option>
                                            </select>
                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Program" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                    </tr>
                                    <tr>
                                        <td class="fieldlabel">Fee Code</td>
                                        <td class="fieldarea">
                                            <select ng-model="SelectedFeeCode"
                                                    ng-change="@(onChangeFeeCode)()"
                                                    ng-options="option.Name for option in FeeCodeList " ng-init="" class="form-control select-inline">
                                                <option value="" label="" class="">---Select Fee Code---</option>
                                            </select>
                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Fee Code" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                    </tr>

                                    @if (!SiteSettings.Instance.AddNewApplicantFormSettings.IsAutoAllocateSyllabus)
                                    {
                                        <tr>
                                            <td class="fieldlabel">Syllabus / Curriculum </td>
                                            <td class="fieldarea">
                                                <select ng-model="Account.@User_Student.Property_CurriculumId"
                                                        ng-options="option.Id as option.Name for option in CurriculumList " ng-init="" class="form-control select-inline">
                                                    <option value="" label="" class="">---Select One---</option>
                                                </select>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Dept Batch." data-toggle="tooltip"></i></span></td>
                                        </tr>
                                    }

                                    @if (SiteSettings.Instance.AddNewApplicantFormSettings.IsEnableDeptBatchSelection)
                                    {
                                        <tr>
                                            <td class="fieldlabel">Dept Batch </td>
                                            <td class="fieldarea">
                                                <select ng-model="Account.@User_Student.Property_ContinuingBatchId"
                                                        ng-options="option.Id as option.Name for option in DeptBatchList " ng-init="" class="form-control select-inline">
                                                    <option value="" label="" class="">---Select One---</option>
                                                </select>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Dept Batch." data-toggle="tooltip"></i></span></td>
                                        </tr>
                                    }
                                    @*<tr>
            <td class="fieldlabel">Class Section <span class="red glyphicon glyphicon-asterisk"></span></td>
            <td class="fieldarea">
                <select ng-model="Account.@User_Student.Property_ClassSectionId"
                        ng-options="option.Id as option.Name for option in ClassSectionList " ng-init="" class="form-control select-inline">
                    <option value="" label="" class="">---Select One---</option>
                </select>
            </td>
            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select ClassSection." data-toggle="tooltip"></i></span></td>
        </tr>*@
                                    @if (SiteSettings.Instance.AddNewApplicantFormSettings.IsEnableEnrollTypeSelection)
                                    {
                                        <tr>
                                            <td class="fieldlabel">Enrollment Type</td>
                                            <td class="fieldarea">
                                                <select ng-model="Account.EnrollmentTypeEnumId"
                                                        ng-change="OnChangeInternalFreshStudent(Account.EnrollmentTypeEnumId==@((byte)User_Student.EnrollmentTypeEnum.InternalPackageStudent)
                                                        ||Account.EnrollmentTypeEnumId==@((byte)User_Student.EnrollmentTypeEnum.TransfereeStudent)); checkEnrollmentType()"
                                                        ng-options="option.Id as option.Name for option in  EnrollmentTypeEnumList " ng-init="" class="form-control select-inline">
                                                    <option value="" label="" class="">---Select One---</option>

                                                </select>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Enrollment Type" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>
                                        <tr ng-show="Account.EnrollmentTypeEnumId == @((byte)User_Student.EnrollmentTypeEnum.InternalDepartmentChange)">
                                            <td class="fieldlabel">Previous Id <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <input ng-enter="GetPreviousStudentInformation()" ng-blur="GetPreviousStudentInformation()" ng-model="Account.PreviousStudentUserName" class="form-control" placeholder="Enter Previous Id" maxlength="128" required="" />
                                                <em ng-show="HasPreviousUserNameError" style="color: red">{{ErrorMsg}}</em>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Previous Id" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>
                                        <tr ng-show="Account.PreviousProgramId!=null && Account.PreviousProgramId!=''">
                                            <td class="fieldlabel">Previous Program <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <select ng-disabled="true" ng-model="Account.PreviousProgramId"
                                                        ng-options="option.Id as option.Name for option in  ProgramList " ng-init="" class="form-control select-inline">
                                                </select>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Previous Program" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>
                                    }
                                    <tr>
                                        <td class="fieldlabel">Gender</td>
                                        <td class="fieldarea">
                                            <label class="radio-inline">
                                                <input ng-disabled="HasPreviousUserNameError" ng-model="Account.GenderEnumId"
                                                       value="@((byte) User_Account.GenderEnum.Male)"
                                                       type="radio" name="latefeeoveride" tabindex="15">Male
                                            </label>
                                            <label class="radio-inline">
                                                <input ng-disabled="HasPreviousUserNameError" ng-model="Account.GenderEnumId"
                                                       value="@((byte) User_Account.GenderEnum.Female)"
                                                       type="radio" name="latefeeoveride" tabindex="15"> Female
                                            </label>



                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Gender" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                    </tr>

                                    @if (!SiteSettings.Instance.AddNewApplicantFormSettings.IsAutoClassSection)
                                    {
                                        <tr>
                                            <td class="fieldlabel">Section</td>
                                            <td class="fieldarea">
                                                <select ng-disabled="HasPreviousUserNameError" ng-model="Account.ClassSectionId"
                                                        ng-change=""
                                                        ng-options="option.Id as option.Name for option in  ClassSectionList " ng-init="" class="form-control select-inline">
                                                    <option value="" label="" class="">---Select One---</option>

                                                </select>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Section" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>
                                    }

                                    @if (SiteSettings.Instance.AddNewApplicantFormSettings.IsEnableCampusSelection)
                                    {
                                        <tr>
                                            <td class="fieldlabel">Campus</td>
                                            <td class="fieldarea">
                                                <select ng-disabled="HasPreviousUserNameError" ng-model="Account.CampusId"
                                                        ng-options="option.Id as option.Name for option in  CampusList " ng-init="" class="form-control select-inline">
                                                    <option value="" label="" class="">---Select One---</option>

                                                </select>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Syllabus" data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>
                                    }

                                    @if (SiteSettings.Instance.AddNewApplicantFormSettings.IsEnableMobileInput)
                                    {
                                        <tr>
                                            <td class="fieldlabel">Mobile No <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <input ng-disabled="HasPreviousUserNameError" ng-show="Account.@User_Account.Property_Id==0" type="text" ng-model="Account.MobileNo" class="form-control" placeholder="Enter Mobile No" maxlength="128" required="" />
                                                <span ng-show="Account.@User_Account.Property_Id!=0" class="padding-l-10 bold-text" style="font-size: 18px;">{{Account.MobileNo}}</span>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Mobile No." data-toggle="tooltip"></i></span></td>
                                        </tr>
                                    }
                                    @if (SiteSettings.Instance.AddNewApplicantFormSettings.IsEnableEmailInput)
                                    {
                                        <tr>
                                            <td class="fieldlabel">Email Address <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <input ng-disabled="HasPreviousUserNameError" ng-show="Account.@User_Account.Property_Id==0" type="text" ng-model="Account.EmailAddress" class="form-control" placeholder="Enter Email Address" maxlength="128" required="" />
                                                <span ng-show="Account.@User_Account.Property_Id!=0" class="padding-l-10 bold-text" style="font-size: 18px;">{{Account.EmailAddress}}</span>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Email Address." data-toggle="tooltip"></i></span></td>
                                        </tr>
                                    }

                                    <tr>
                                        <td class="fieldlabel">Student ID <span class="red glyphicon glyphicon-asterisk"></span></td>
                                        <td class="fieldarea">

                                            <div ng-show="Account.@User_Account.Property_Id==0" class="input-group" style="width: 100%;">
                                                @if (SiteSettings.Instance.StudentIdGenerateType != SiteSettings.StudentIdGenerateTypeEnum.FullManual)
                                                {
                                                    if (SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerate
                                                        || SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.FullAutoGenerate)
                                                    {
                                                        <span ng-show="!Account.IsFullManualId"
                                                              class="input-group-addon bold-text">{{SelectedAdmissionExam.StudentIdPrefix+SelectedProgram.DepartmentCode}}</span>
                                                    }
                                                    else
                                                    {
                                                        <span ng-show="!Account.IsFullManualId"
                                                              class="input-group-addon bold-text">{{SelectedAdmissionExam.UgcIdPrefix+SelectedProgram.UgcCode}}</span>
                                                    }
                                                }

                                                @if (SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerate || SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerateAsUGC)
                                                {
                                                    <input ng-disabled="HasPreviousUserNameError" type="text"
                                                           ng-model="Account.@User_Account.Property_UserName"
                                                           ng-show="!Account.IsFullManualId"
                                                           class="form-control "
                                                           placeholder="@((SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerate
                                                               || SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.PrefixAutoGenerateAsUGC) ?
                                                                            "Last " + SiteSettings.Instance.StudentIdSerialDigitCount + " Digit" : SiteSettings.Instance.StudentIdSerialDigitCount + " Digit Student ID")"
                                                           maxlength="@(SiteSettings.Instance.StudentIdSerialDigitCount)" required="" />
                                                }
                                                @if (SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.FullAutoGenerate || SiteSettings.Instance.StudentIdGenerateType == SiteSettings.StudentIdGenerateTypeEnum.FullAutoGenerateAsUGC)
                                                {
                                                    <input type="text"
                                                           disabled="disabled"
                                                           @*ng-model="Account.@User_Account.Property_UserName"*@
                                                           ng-show="!Account.IsFullManualId"
                                                           class="form-control "
                                                           placeholder="@("Last " + SiteSettings.Instance.StudentIdSerialDigitCount + " Digit Auto")" />
                                                }

                                                <input ng-disabled="HasPreviousUserNameError" type="text"
                                                       ng-model="Account.@User_Account.Property_UserName"
                                                       ng-show="Account.IsFullManualId"
                                                       class="form-control "
                                                       placeholder="Manual Student ID" />
                                            </div>

                                            <span ng-show="Account.@User_Account.Property_Id!=0" class="padding-l-10 bold-text" style="font-size: 18px;">{{Account.UserName}}</span>

                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Please type only last @(SiteSettings.Instance.StudentIdSerialDigitCount) Digit." data-toggle="tooltip"></i></span></td>
                                    </tr>
                                    <tr>
                                        <td class="fieldlabel"></td>
                                        <td class="fieldarea" style="font-size: 11px; padding: 2px 20px;">
                                            <span ng-show="Account.MaxUserName!='' && Account.AdmissionExamId >0 && Account.ProgramId >0" class="padding-l-10">
                                                Last Max ID: {{Account.MaxUserName}}
                                            </span>
                                            <span ng-show="Account.MaxUserName=='' && Account.AdmissionExamId >0 && Account.ProgramId >0" class="padding-l-10">
                                                [No Student Admitted]
                                            </span>
                                            <button ng-show="Account.AdmissionExamId >0 && Account.ProgramId >0"
                                                    ng-click="GetMaxAndSuggestUsername()"
                                                    style="margin-left: 10px; padding: 2px 0px 0px 2px;"
                                                    class="btn btn-default btn-sm">
                                                <i class="fa fa-refresh"></i>
                                            </button>

                                            @if (SiteSettings.Instance.StudentIdGenerateType != SiteSettings.StudentIdGenerateTypeEnum.FullManual)
                                            {
                                                <span style="float: right;">
                                                    |<input ng-model="Account.IsFullManualId" style="width: 12px; vertical-align: middle;" type="checkbox" />
                                                    <span>Full Manual ID</span>
                                                </span>

                                            }

                                        </td>

                                        <td class="fieldarea-tooltip"></td>
                                    </tr>

                                    <tr>
                                        <td class="fieldlabel">Full Name <span class="red glyphicon glyphicon-asterisk"></span></td>
                                        <td class="fieldarea">
                                            <input ng-disabled="HasPreviousUserNameError" ng-show="Account.@User_Account.Property_Id==0" type="text" ng-model="Account.@User_Account.Property_FullName" class="form-control" placeholder="Enter Full Name" maxlength="128" required="" />
                                            <span ng-show="Account.@User_Account.Property_Id!=0" class="padding-l-10 bold-text" style="font-size: 18px;">{{Account.@User_Account.Property_FullName}}</span>
                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Full Name." data-toggle="tooltip"></i></span></td>
                                    </tr>
                                    @if (SiteSettings.Instance.AddNewApplicantFormSettings.IsEnablePreviousEducationInput)
                                    {
                                        <tr ng-show="SelectedFeeCode != null">
                                            <td colspan="3">
                                                <table>
                                                    <tr>
                                                        <td class="fieldarea" colspan="4" style="text-align: center;font-size: 14px;font-weight: bold;">
                                                            SSC/Equivalent Information
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td class="fieldlabel" style="width: 140px;">Degree Category<span class="red glyphicon glyphicon-asterisk"></span></td>
                                                        <td class="fieldarea" style="width: 140px;">
                                                            <select ng-disabled="Account.@(User_Account.Property_Id)!=0 || HasPreviousUserNameError"
                                                                    ng-model="Account.SscDegreeCategoryId"
                                                                    ng-options="option.Id as option.Name for option in  SscEquivalentDegreeCategoryList "
                                                                    class="form-control select-inline">
                                                                <option value="" label="" class="">---Select One---</option>
                                                            </select>
                                                        </td>
                                                        <td class="fieldlabel" style="width: 60px;">GPA<span class="red glyphicon glyphicon-asterisk"></span></td>
                                                        <td class="fieldarea">
                                                            <input type="number" ng-model="Account.SscGpa"
                                                                   ng-disabled="Account.@(User_Account.Property_Id)!=0 || HasPreviousUserNameError"
                                                                   ng-change="onChangeSscHscGpa()"
                                                                   class="form-control" placeholder="SSC GPA" />
                                                            <div ng-show="Account.SscGpa==5 || Account.@(User_Account.Property_Id)!=0">
                                                                <input ng-model="Account.SscIsGolden"
                                                                       ng-change="onChangeSscHscIsGolden()"
                                                                       ng-disabled="Account.SscGpa!=5 || Account.@(User_Account.Property_Id)!=0"
                                                                       style="width: 12px; vertical-align: middle;" type="checkbox">
                                                                <span>SSC Golden</span>
                                                            </div>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td class="fieldarea" colspan="4" style="text-align: center;font-size: 14px;font-weight: bold;">
                                                            HSC/Equivalent Information
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td class="fieldlabel" style="width: 140px;">Degree Category</td>
                                                        <td class="fieldarea" style="width: 140px;">
                                                            <select ng-disabled="Account.@(User_Account.Property_Id)!=0 || HasPreviousUserNameError"
                                                                    ng-model="Account.HscDegreeCategoryId"
                                                                    ng-options="option.Id as option.Name for option in HscEquivalentDegreeCategoryList"
                                                                    class="form-control select-inline">
                                                                <option value="" label="" class="">---Select One---</option>
                                                            </select>
                                                        </td>
                                                        <td class="fieldlabel" style="width: 60px;">GPA</td>
                                                        <td class="fieldarea">
                                                            <input type="number" ng-model="Account.HscGpa"
                                                                   ng-disabled="Account.@(User_Account.Property_Id)!=0 || HasPreviousUserNameError"
                                                                   ng-change="onChangeSscHscGpa()"
                                                                   class="form-control" placeholder="HSC GPA" max="5" />
                                                            <div ng-show="Account.HscGpa==5 || Account.@(User_Account.Property_Id)!=0">
                                                                <input ng-model="Account.HscIsGolden"
                                                                       ng-change="onChangeSscHscIsGolden()"
                                                                       ng-disabled="Account.HscGpa!=5 || Account.@(User_Account.Property_Id)!=0"
                                                                       style="width: 12px; vertical-align: middle;" type="checkbox">
                                                                <span>HSC Golden</span>
                                                            </div>


                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td class="fieldarea" colspan="4" style="text-align: center; font-size: 16px; font-weight: bold;">
                                                            Average GPA: {{Account.AverageGpa| number:2}} <span ng-show="Account.AverageIsGolden">(Golden)</span>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                    }

                                    @if (SiteSettings.Instance.AddNewApplicantFormSettings.IsEnableRemarkInput)
                                    {
                                        <tr>
