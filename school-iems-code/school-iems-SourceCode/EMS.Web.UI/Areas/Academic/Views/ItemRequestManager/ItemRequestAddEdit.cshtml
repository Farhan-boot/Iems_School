

@section ScriptSection
{
    <script src="~/Areas/Academic/Views/ItemRequestManager/ItemRequestManager.js"></script>
}


    <!--File:BAcnt_Voucher Add/Edit View-->
    <div id="ItemRequestAddEdit" class="row ng-scope" ng-controller="ItemRequestAddEditCtrl" ng-init="Init(
      );">

        <div class="col-lg-12 col-sm-12 col-xs-12 voucher-entry-form">
            <div ng-hide="!HasError ||ErrorMsg==''" class="alert alert-danger fade in radius-bordered alert-shadowed ng-binding ng-hide">
                <button ng-click="HasError=false" class="close" data-dismiss1="alert">×</button>
                <i class="fa-fw fa fa-warning"></i>
                <strong>Attention Please!</strong>
            </div>
            <div class="">
                <div class="widget">
                    <div class="widget-header bordered-bottom bordered-sky ">
                        <i class="widget-icon fa fa-edit bold  "></i>
                        <span class="widget-caption ">
                            Add/Edit

                        </span>
                        <div class="widget-buttons buttons-bordered no-padding-right" style="display: none">
                            <span class="input-icon">
                                <input type="text" ng-model="VoucherId" ng-enter="getVoucherById()" class="form-control input-xs  ng-pristine ng-untouched ng-valid" id="glyphicon-search" placeholder="Search">
                                <i class="glyphicon glyphicon-search blue"></i>
                            </span>

                        </div>
                        <div class="widget-buttons no-padding-left">
                            <button ng-click="getNewVoucher()" class="btn btn-success btn-xs"><i class="fa fa-plus"></i> Add New</button>
                            <a href="/BasicAccounting/Voucher/VoucherList" class="btn btn-blue btn-xs"><i class="fa fa-arrow-left" target="_blank"></i> Back To List</a>
                            <button ng-click="loadPage()" class="btn btn-info btn-xs"><i class="fa fa-refresh"></i> Refresh</button>
                        </div>
                    </div><!--Widget Header end-->
                    <div class="widget-body">
                        <div class="row">
                            <div class="col-md-6 table-form">
                                <table class="form" width="100%" border="0" cellspacing="2" cellpadding="3">
                                    <tbody>
                                        <tr>
                                            <td class="fieldlabel">Person Name <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <input type="text" ng-model="ItemRequest.PersonName" class="form-control ng-pristine ng-invalid ng-invalid-required ng-valid-maxlength ng-touched" placeholder="Enter Person Name" maxlength="50" required="">
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Person Name." data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>
                                        <tr>
                                            <td class="fieldlabel">Ishu Date <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <input type="text" ng-model="ItemRequest.IshuDate" class="form-control date-picker ng-pristine ng-valid ng-valid-datetime ng-valid-min ng-valid-max ng-touched" placeholder="Enter Ishu Date" datetime="dd/MM/yyyy">
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Ishu Date." data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>
                                        <tr>
                                            <td class="fieldlabel">Item Name <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <select ng-model="ItemRequest.ItemId" ng-options="" ng-init="" class="form-control select-inline ng-valid ng-dirty ng-valid-parse ng-touched">
                                                    <option value="" label="" class="">---Select One---</option>
                                                    <option value="0" label="Foot Ball">Foot Ball</option>
                                                    <option value="1" label="Cricket Bat">Cricket Bat</option>
                                                </select>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Voucher Type." data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>


                                        <!--dropdown option for EnumList-->

                                        <tr>
                                            <td class="fieldlabel">Catagory Name </td>
                                            <td class="fieldarea">
                                                <input readonly type="text" ng-model="ItemRequest.CatagoryId" class="form-control ng-pristine ng-untouched ng-invalid ng-invalid-required ng-valid-maxlength" placeholder="Catagory Name" maxlength="50" required="">
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Catagory Name." data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>

                                    </tbody>
                                </table>

                            </div><!--Col end-->
                            <div class="col-md-6 table-form">
                                <table class="form" width="100%" border="0" cellspacing="2" cellpadding="3">
                                    <tbody>
                                        <tr>
                                            <td class="fieldlabel">Return Date <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <input type="text" ng-model="ItemRequest.ReturnDate" class="form-control date-picker ng-pristine ng-valid ng-valid-datetime ng-valid-min ng-valid-max ng-touched" placeholder="Enter Return Date" datetime="dd/MM/yyyy">
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Return Date." data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>

                                        <tr>
                                            <td class="fieldlabel">Unit Type <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <select ng-model="ItemRequest.UnitType" ng-options="" ng-init="" class="form-control select-inline ng-valid ng-dirty ng-valid-parse ng-touched">
                                                    <option value="" label="" class="">---Select One---</option>
                                                    <option value="0" label="Pic">Pic</option>
                                                    <option value="0" label="Kg">Kg</option>
                                                </select>
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Unit Type." data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>


                                        <tr>
                                            <td class="fieldlabel">Unit <span class="red glyphicon glyphicon-asterisk"></span></td>
                                            <td class="fieldarea">
                                                <input type="text" ng-model="ItemRequest.Unit" class="form-control ng-pristine ng-valid ng-valid-maxlength ng-touched" placeholder="Enter Unit" maxlength="255">
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Unit." data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>

                                        <tr>
                                            <td class="fieldlabel">Remark </td>
                                            <td class="fieldarea">
                                                <input type="text" ng-model="ItemRequest.Remark" class="form-control ng-pristine ng-valid ng-valid-maxlength ng-touched" placeholder="Enter Remark" maxlength="255">
                                            </td>
                                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Remark." data-toggle="tooltip" data-original-title="" title=""></i></span></td>
                                        </tr>
                                    </tbody>
                                </table>

                            </div><!--Col end-->


                            <div class="col-md-12 text-center">
                                <button ng-click="saveItemRequest()" class="btn btn-md btn-success"><span class="fa fa-save"></span> Save</button>
                            </div>



                        </div><!--Row end-->
                    </div><!--Widget Body end-->


                    <div class="widget-body no-padding" style="overflow:auto">
                        <div class="table-footer no-print">
                            <div class="row no-print">
                                <div class="col-sm-4">
                                    <div class="form-inline pull-left">
                                        <div class="form-group">
                                            <label ng-hide="totalServerItems==0" class="ng-binding">(Showing 1-11 of 11)</label>
                                            <label ng-show="totalServerItems==0" class="text-danger ng-hide">No Data Found!</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-8">
                                    <div class="form-inline pull-right">
                                        <div class="form-group">
                                            <label>Page Size</label>
                                            <input ng-model="PageSize" ng-enter="changePageSize()" class="form-control input-opt ng-pristine ng-untouched ng-valid ng-valid-min" autocomplete="on" placeholder="Page Size" type="number" min="1">
                                        </div>
                                        <div class="form-group">
                                            <label>Page No.</label>
                                            <input ng-model="PageNo" ng-enter="changePageNo()" class="form-control input-opt ng-pristine ng-untouched ng-valid ng-valid-min" autocomplete="on" placeholder="Page No" type="number" min="1">
                                            <label class="ng-binding">of 0</label>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <table class="table table-striped table-bordered table-hover">
                            <thead class="bordered-blue">
                                <tr>
                                    <th class="text-center" style="display:none">
                                        <input type="checkbox" ng-click="selectAll($event)">
                                    </th>
                                    <th class="text-center">SN</th>
                                    <th class="text-center">Person Name</th>
                                    <th class="text-center">Ishu Date</th>
                                    <th class="text-center">Item</th>
                                    <th class="text-center">Catagory</th>
                                    <th class="text-center">Return Date</th>
                                    <th class="text-center">Unit Type</th>
                                    <th class="text-center">Unit</th>
                                    <th class="text-center">Status</th>
                                    <th class="text-center">Remark</th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tbody>

                                <!-- ngRepeat: row in GradingPolicyOptionList | orderBy:'SerNo' -->
                                <tr class="ng-scope">
                                    <td class="text-center" style="display:none">
                                        <input type="checkbox" ng-model="row.IsSelected" class="ng-pristine ng-untouched ng-valid">
                                    </td>
                                    <td class="text-center ng-binding">1</td>
                                    <td class="ng-binding">Farhan</td>
                                    <td class="ng-binding">10/10/2022</td>
                                    <td class="ng-binding">Cricket Bat</td>
                                    <td class="ng-binding">Sports</td>
                                    <td class="ng-binding">10/12/2023</td>
                                    <td class="ng-binding">Pic</td>
                                    <td class="ng-binding">10</td>
                                    <td class="ng-binding">Panding</td>
                                    <td class="ng-binding">Remark....</td>

                                    <td class="text-center">
                                        <a target="_blank" href="#" class="btn btn-xs btn-primary"><i class="fa fa-edit"></i> Edit</a>
                                        <button ng-click="deleteItemRequest(row)" class="btn btn-xs btn-danger"><i class="fa fa-trash"></i></button>
                                    </td>
                                </tr><!-- end ngRepeat: row in GradingPolicyOptionList | orderBy:'SerNo' -->
                                <tr class="ng-scope">
                                    <td class="text-center" style="display:none">
                                        <input type="checkbox" ng-model="row.IsSelected" class="ng-pristine ng-untouched ng-valid">
                                    </td>
                                    <td class="text-center ng-binding">2</td>
                                    <td class="ng-binding">Sakib</td>
                                    <td class="ng-binding">10/10/2022</td>
                                    <td class="ng-binding">Foot ball</td>
                                    <td class="ng-binding">Sports</td>
                                    <td class="ng-binding">10/12/2023</td>
                                    <td class="ng-binding">Pic</td>
                                    <td class="ng-binding">15</td>
                                    <td class="ng-binding">Panding</td>
                                    <td class="ng-binding">Remark....</td>

                                    <td class="text-center">
                                        <a target="_blank" href="#" class="btn btn-xs btn-primary"><i class="fa fa-edit"></i> Edit</a>
                                        <button ng-click="deleteItemRequest(row)" class="btn btn-xs btn-danger"><i class="fa fa-trash"></i></button>
                                    </td>
                                </tr><!-- end ngRepeat: row in GradingPolicyOptionList | orderBy:'SerNo' -->
                               
                            </tbody>
                        </table>
                        <div class="table-footer no-print">
                            <div class="row no-print">
                                <div class="col-sm-4">
                                    <div class="form-inline pull-left">
                                        <div class="form-group">
                                            <label ng-hide="totalServerItems==0" class="ng-binding">(Showing 1-11 of 11)</label>
                                            <label ng-show="totalServerItems==0" class="text-danger ng-hide">No Data Found!</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-8">
                                    <div class="form-inline pull-right">
                                        <div class="form-group">
                                            <label>Page Size</label>
                                            <input ng-model="PageSize" ng-enter="changePageSize()" class="form-control input-opt ng-pristine ng-untouched ng-valid ng-valid-min" autocomplete="on" placeholder="Page Size" type="number" min="1">
                                        </div>
                                        <div class="form-group">
                                            <label>Page No.</label>
                                            <input ng-model="PageNo" ng-enter="changePageNo()" class="form-control input-opt ng-pristine ng-untouched ng-valid ng-valid-min" autocomplete="on" placeholder="Page No" type="number" min="1">
                                            <label class="ng-binding">of 0</label>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>




                </div>
            </div><!--view permission end-->
        </div>
    </div><!--controller end-->


    <style>

        .voucher-entry-form input,
        .voucher-entry-form optgroup,
        .voucher-entry-form select,
        .voucher-entry-form textarea {
            height: 25px !important;
            font-size: 12px;
        }

        .text-dropdown-toggle .dropdown-menu li a:hover {
            background: #1884e6;
            color: #fff;
        }

        .text-dropdown {
            max-height: 115px;
            overflow-y: auto;
            font-size: 13px;
            padding: 6px 12px;
            width: 100%;
            line-height: 1.42857143;
            background-image: none;
        }

        .text-dropdown-toggle ul {
            max-height: 115px !important;
            padding: 0px !important;
            overflow-x: hidden !important;
        }

            .text-dropdown-toggle ul li a {
                padding-left: 0px !important;
                padding-right: 0px !important;
                padding: 2px 10px !important;
            }
    </style>




