
@using EMS.DataAccess.Data;
@using EMS.Framework.Settings
@using EMS.Web.UI.Areas.BasicAccounting.Controllers
@model EMS.Framework.Objects.MvcModelResult<ReportController.ReceiptPaymentModel>
@{
    ViewBag.Title = "Receipt Payment";
    Layout = "~/Areas/BasicAccounting/Views/Shared/_LayoutBasicAccountingReport.cshtml";

}


@section ReportHeaderSection
{
    @Html.Partial("~/Views/Shared/Report/_ReportHeader.cshtml")
}

@Html.Partial("~/Areas/BasicAccounting/Views/Report/Shared/_ReceiptPaymentPrint.cshtml",Model)

@section ReportFooterSection
{
    <div style="margin-top: 30px;">
        <table style="width: 100%; margin-top: 5px">
            <tbody>
            <tr style="color: gray !important;">
                <td>Developed By: @SiteSettings.PoweredBy</td>
                
                <td style="text-decoration: overline;">
                    Checked by
                </td>

                <td style="text-align: right;">
                  
                    @SiteSettings.Instance.InstituteCreditPrefix @SiteSettings.Instance.InstituteCredit
                
                </td>

            </tr>
            </tbody>
        </table>
    </div>
    @*@Html.Partial("~/Views/Shared/Report/_ReportFooter.cshtml")*@
}