                                    <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Retake Exam Fee Per Course." data-toggle="tooltip"></i></span></td>
                                </tr>


                                </tbody>
                            </table>
                        </div><!--Col end-->
                        <div class="col-md-6 table-form">
                            <table class="form" width="100%" border="0" cellspacing="2" cellpadding="3">
                                <tbody>
                                    <tr>
                                        <td class="fieldlabel">Total Credit </td>
                                        <td class="fieldarea">
                                            <input type="number" ng-change="calculatePackage()" ng-model="FeeCode.@Acnt_FeeCode.Property_TotalCredit" class="form-control" placeholder="Enter Total Credit" />
                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Total Credit." data-toggle="tooltip"></i></span></td>
                                    </tr>
                                    <tr>
                                        <td class="fieldlabel">Total Semester </td>
                                        <td class="fieldarea">
                                            <input type="number" ng-change="calculatePackage()" ng-model="FeeCode.@Acnt_FeeCode.Property_TotalSemester" class="form-control" placeholder="Enter Total Semester" />
                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Total Semester." data-toggle="tooltip"></i></span></td>
                                    </tr>
                                    <tr>
                                        <td class="fieldlabel">Total Year </td>
                                        <td class="fieldarea">
                                            <input type="number" ng-change="calculatePackage()" ng-model="FeeCode.@Acnt_FeeCode.Property_TotalYear" class="form-control" placeholder="Enter Total Year" />
                                        </td>
                                        <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Total Year." data-toggle="tooltip"></i></span></td>
