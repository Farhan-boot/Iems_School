            @*Fee Code Policy Start*@
        <div class="alert alert-danger fade in radius-bordered alert-shadowed"
             ng-if="FeeCode.@Acnt_FeeCode.Property_Id !=0"
             ng-hide="(TotalAmount|number:2)==(FeeCode.@Acnt_FeeCode.Property_TotalPackageAmount|number:2)">
                <i class="fa-fw fa fa-warning"></i>
                <strong>Total Package Amount Mismatch!</strong> Total Package Amount({{FeeCode.@Acnt_FeeCode.Property_TotalPackageAmount|number:2}}) & Calculated Total Amount({{TotalAmount|number:2}}) Not Same Please Check!
            </div>

            <div class="widget" ng-if="FeeCode.@Acnt_FeeCode.Property_Id !=0">
                <div class="widget-header bordered-bottom bordered-sky ">
                    <i class="widget-icon fa fa-list bold  "></i>
                    <span class="widget-caption ">
                        Manage Fee Code Policy (<span class="bold-text">
                            Calculated Amount To Pay:
                            <label class="label label-purple">{{FeeCodePolicyList|total:'Total'|number:2 }}</label>
                        </span>)
                    </span>


                    <div class="widget-buttons no-padding-left">
                        <button ng-click="@(getNewFeeCodePolicyModal)()" class="btn btn-xs btn-success"><i class="fa fa-plus"></i> Add New</button>
                        <button ng-click="@(getAllFeeCodePolicyList)()" class="btn btn-info btn-xs"><i class="fa fa-refresh"></i> Refresh</button>
                    </div>
                </div>
                <div class="widget-body no-padding" style="overflow: auto">
                    <table class="table table-striped table-bordered table-hover">
                        <thead class="bordered-blue">
                            <tr>
                                <th class="text-center" style="display: none">
                                    <input type="checkbox" ng-click="selectAll($event)">
                                </th>
                                <th class="text-center">SN</th>
                                <th class="text-center">Name</th>
                                <!--th class="text-center">Fee Code Id</th-->
                                <!--th class="text-center">Particular Type Id</th-->
                                <!--th class="text-center">Particular Type Enum Id</th-->
                                <th class="text-center">Particular Type</th>
                                <!--th class="text-center">Fee Generate Term Enum Id</th-->

                                <th class="text-center">Auto Generate</th>

                                <th class="text-center">Order No</th>
                                <th class="text-center">Fee Generate Term</th>
                                <th class="text-center">Amount</th>
                                <th class="text-center">Equation</th>
                                <th class="text-center">Total</th>
                                <th class="text-center no-print">Action</th>
                            </tr>
                        </thead>
                        <tbody>

                            <tr ng-repeat="row in FeeCodePolicyList">
                                <td class="text-center" style="display: none">
                                    <input type="checkbox" ng-model="row.@Acnt_FeeCodePolicy.Property_IsSelected" />
                                </td>
                                <td class="text-center">{{$index+1}}</td>
                                <td class="">{{row.@Acnt_FeeCodePolicy.Property_ParticularTypeName}}</td>
                                <!--td class="">{{row.@Acnt_FeeCodePolicy.Property_FeeCodeId}}</td-->
                                <!--td class="">{{row.@Acnt_FeeCodePolicy.Property_ParticularNameId}}</td-->
                                <!--td class="">{{row.@Acnt_FeeCodePolicy.Property_ParticularTypeEnumId}}</td-->
                                <td class="">{{row.@Acnt_FeeCodePolicy.Property_ParticularType}}</td>
                                <!--td class="">{{row.@Acnt_FeeCodePolicy.Property_FeeGenerateTermEnumId}}</td-->

                                <td class="">{{row.@Acnt_FeeCodePolicy.Property_IsAutoGenerateOnceOnCourseAdd}}</td>
                                @*<td class="">{{row.@Acnt_FeeCodePolicy.Property_Remark}}</td>*@
                                <td class="">{{row.@Acnt_FeeCodePolicy.Property_OrderNo}}</td>
                                <td class="">{{row.@Acnt_FeeCodePolicy.Property_FeeGenerateTerm}}</td>
                                @*<td class="">{{row.@Acnt_FeeCodePolicy.Property_IsAmountInPercentage}}</td>*@
                                <td class="">{{row.@Acnt_FeeCodePolicy.Property_Amount}}</td>
                                <td class="">{{row.@Acnt_FeeCodePolicy.Property_EquationTxt}}</td>
                                <td class="">{{row.@Acnt_FeeCodePolicy.Property_Total|number:2}}</td>

                                <td class="text-center no-print">
                                    <button ng-click="@(getFeeCodePolicyByIdModal)(row.Id)" class="btn btn-xs btn-primary"><i class="fa fa-edit"></i> Edit</button>
                                    <button ng-click="@(deleteFeeCodePolicyById)(row)" class="btn btn-xs btn-danger"><i class="fa fa-trash"></i></button>

                                </td>
                            </tr>

                        </tbody>
                        <tfoot class="table-footer ">
                            <tr>
                                <td colspan="8" class="text-right bold-text">Calculated Amount To Pay</td>
                                <td colspan="2" class="bold-text">{{FeeCodePolicyList|total:'Total'|number:2 }}</td>

                            </tr>
                        </tfoot>
                    </table>

                </div>
            </div>
            <!--Component Modal -->
            <div class="modal fade" id="FeeCodePolicyModal" tabindex="-1" role="dialog" aria-labelledby="FeeCodePolicyModal">
                <div class="modal-dialog modal-m" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title" id="">Add/Edit Fee Code Policy</h4>
