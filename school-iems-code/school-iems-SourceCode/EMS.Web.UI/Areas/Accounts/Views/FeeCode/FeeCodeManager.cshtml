<!--File:Acnt_FeeCode Add/Edit View-->
@using EMS.DataAccess.Data;
@{
    ViewBag.Title = "Accounts: Add/Edit Fee Code";
    Layout = "~/Areas/Accounts/Views/Shared/_LayoutAccounts.cshtml";
    // Angular Properties
    var id = Request.QueryString["id"];//ViewContext.RouteData.Values["id"];
    var FeeCodeId = (string)id == null ? "0" : (string)id;

    //WebApi link for single row
    var getFeeCodeByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodeApi", action = "GetFeeCodeById", area = "Accounts" });
    var getFeeCodeDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodeApi", action = "GetFeeCodeDataExtra", area = "Accounts" });
    var saveFeeCodeUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodeApi", action = "SaveFeeCode", area = "Accounts" });
    var deleteFeeCodeByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodeApi", action = "GetDeleteFeeCodeById", area = "Accounts" });

    //WebApi link for List
    var getPagedFeeCodeUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodeApi", action = "GetPagedFeeCode", area = "Accounts" });
    var getAllFeeCodeUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodeApi", action = "GetAllFeeCode", area = "Accounts" });
    var saveFeeCodeListUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodeApi", action = "SaveFeeCodeList", area = "Accounts" });
    var getFeeCodeListDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodeApi", action = "GetFeeCodeListDataExtra", area = "Accounts" });

    var editFeeCodeByIdPageUrl = Url.Action("FeeCodeAddEdit", "FeeCode", new { area = "Accounts" });
    var listFeeCodePageUrl = Url.Action("FeeCodeList", "FeeCode", new { area = "Accounts" });

    // WebApi For FeeCodePolicy List
    var getFeeCodePolicyListByFeeCodeIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodePolicyApi", action = "GetFeeCodePolicyListByFeeCodeId", area = "Accounts" });
    var saveFeeCodePolicyListUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodePolicyApi", action = "SaveFeeCodePolicyList", area = "Accounts" });
    var getFeeCodePolicyListDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodePolicyApi", action = "GetFeeCodePolicyListDataExtra", area = "Accounts" });

    //WebApi link for FeeCodePolicy single row
    var getFeeCodePolicyByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodePolicyApi", action = "GetFeeCodePolicyById", area = "Accounts" });
    //var getFeeCodePolicyDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodePolicyApi", action = "GetFeeCodePolicyDataExtra", area = "Accounts" });
    var saveFeeCodePolicyUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodePolicyApi", action = "SaveFeeCodePolicy", area = "Accounts" });
    var deleteFeeCodePolicyByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "FeeCodePolicyApi", action = "GetDeleteFeeCodePolicyById", area = "Accounts" });


    var editFeeCodePolicyByIdPageUrl = Url.Action("FeeCodePolicyAddEdit", "FeeCodePolicy", new { area = "Accounts" });


    // Angular Functions
    string getFeeCodeById = "getFeeCodeById";
    var saveFeeCode = "saveFeeCode";
    var getNewFeeCode = "getNewFeeCode";
    var refreshPage = "loadPage()";
    string deleteFeeCodeById = "deleteFeeCodeById";
    string loadFeeCodeExtraData = "loadFeeCodeExtraData";

    // Angular Functions FeeCodePolicy
    var getNewFeeCodePolicyModal = "getNewFeeCodePolicyModal";
    var getFeeCodePolicyByIdModal = "getFeeCodePolicyByIdModal";
    var saveFeeCodePolicy = "saveFeeCodePolicy";
    var deleteFeeCodePolicyById = "deleteFeeCodePolicyById";
    var deleteFeeCodePolicyByObj = "deleteFeeCodePolicyByObj";
    //for list
    //var searchFeeCodePolicyList = "searchFeeCodePolicyList";
    var getPagedFeeCodePolicyList = "getPagedFeeCodePolicyList";
    var getAllFeeCodePolicyList = "getFeeCodePolicyList";
    var saveFeeCodePolicyList = "saveFeeCodePolicyList";
    var getFeeCodePolicyListExtraData = "getFeeCodePolicyListExtraData";

}
@section ScriptSection
{
    <script src="~/Areas/Accounts/Views/FeeCode/FeeCodeManager.js?v=2.1"></script>
}

