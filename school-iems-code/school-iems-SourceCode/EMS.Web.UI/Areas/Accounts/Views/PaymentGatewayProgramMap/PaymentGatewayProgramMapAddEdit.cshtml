
<!--File:Acnt_PaymentGatewayProgramMap Add/Edit View-->
@using EMS.DataAccess.Data;
@{
    ViewBag.Title = "Accounts: Add/Edit Payment Gateway Program Map";
    Layout = "~/Areas/Accounts/Views/Shared/_LayoutAccounts.cshtml";
    // Angular Properties
    var id = Request.QueryString["id"];//ViewContext.RouteData.Values["id"];
    var PaymentGatewayProgramMapId = (string) id == null?"0":(string)id;
    
    //Acnt_PaymentGatewayProgramMap WebApi Single Row link   
    var getPaymentGatewayProgramMapByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "PaymentGatewayProgramMapApi", action = "GetPaymentGatewayProgramMapById", area = "Accounts" });
    var getPaymentGatewayProgramMapDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "PaymentGatewayProgramMapApi", action = "GetPaymentGatewayProgramMapDataExtra", area = "Accounts" });
    var savePaymentGatewayProgramMapUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "PaymentGatewayProgramMapApi", action = "SavePaymentGatewayProgramMap", area = "Accounts" });
    var deletePaymentGatewayProgramMapByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "PaymentGatewayProgramMapApi", action = "GetDeletePaymentGatewayProgramMapById", area = "Accounts" });

    //Acnt_PaymentGatewayProgramMap WebApi List link 
    var getPagedPaymentGatewayProgramMapUrl = Url.HttpRouteUrl("DefaultApi", new {controller = "PaymentGatewayProgramMapApi", action = "GetPagedPaymentGatewayProgramMap", area = "Accounts"});
    var getAllPaymentGatewayProgramMapUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "PaymentGatewayProgramMapApi", action = "GetAllPaymentGatewayProgramMap", area = "Accounts" });
    var savePaymentGatewayProgramMapListUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "PaymentGatewayProgramMapApi", action = "SavePaymentGatewayProgramMapList", area = "Accounts" });
    var getPaymentGatewayProgramMapListDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "PaymentGatewayProgramMapApi", action = "GetPaymentGatewayProgramMapListDataExtra", area = "Accounts" });

    var editPaymentGatewayProgramMapByIdPageUrl = Url.Action("PaymentGatewayProgramMapAddEdit", "PaymentGatewayProgramMap", new { area = "Accounts" });
    var listPaymentGatewayProgramMapPageUrl = Url.Action("PaymentGatewayProgramMapList", "PaymentGatewayProgramMap", new { area = "Accounts" });
    
    //Acnt_PaymentGatewayProgramMap Angular Functions
    string getPaymentGatewayProgramMapById="getPaymentGatewayProgramMapById";
    var savePaymentGatewayProgramMap = "savePaymentGatewayProgramMap";
    var getNewPaymentGatewayProgramMap = "getNewPaymentGatewayProgramMap";
    var refreshPage = "loadPage()";
    string deletePaymentGatewayProgramMapById="deletePaymentGatewayProgramMapById";
    string loadPaymentGatewayProgramMapExtraData="loadPaymentGatewayProgramMapExtraData";
}
@section ScriptSection
{
    <script src="~/Areas/Accounts/Views/PaymentGatewayProgramMap/PaymentGatewayProgramMapAddEdit.js"></script>
}

<div id="PaymentGatewayProgramMapAddEdit" class="row" ng-controller="PaymentGatewayProgramMapAddEditCtrl" ng-init="Init(
      '@PaymentGatewayProgramMapId'
      ,'@getPaymentGatewayProgramMapByIdUrl'
      ,'@getPaymentGatewayProgramMapDataExtraUrl'
      ,'@savePaymentGatewayProgramMapUrl'
      ,'@deletePaymentGatewayProgramMapByIdUrl')">
      
    <div class="col-lg-12 col-sm-12 col-xs-12">
     <div ng-Hide="!HasError ||ErrorMsg==''" class="alert alert-danger fade in radius-bordered alert-shadowed">
            <button ng-click="HasError=false" class="close" data-dismiss1="alert">×</button>
            <i class="fa-fw fa fa-warning"></i>
            <strong>Attention Please!</strong> {{ErrorMsg}}
      </div>
      <div ng-show="HasViewPermission">
        <div class="widget" >
            <div class="widget-header bordered-bottom bordered-sky ">
                <i class="widget-icon fa fa-edit bold  "></i>
                <span class="widget-caption ">
                    Add/Edit
                    @*<strong ng-show="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_Name!=''">{{(PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_Name)}}</strong>*@
                </span>
                <div class="widget-buttons buttons-bordered no-padding-right" style="display: none">
                    <span class="input-icon">
                        <input type="text" ng-model="PaymentGatewayProgramMapId" ng-Enter="@(getPaymentGatewayProgramMapById)()" class="form-control input-xs " id="glyphicon-search" placeholder="Search" >
                        <i class="glyphicon glyphicon-search blue"></i>
                    </span>
                  
                </div>
                <div class="widget-buttons no-padding-left">
                    <button ng-click="@(getNewPaymentGatewayProgramMap)()" class="btn btn-success btn-xs"><i class="fa fa-plus"></i> Add New</button>
                    <a href="@(listPaymentGatewayProgramMapPageUrl)" class="btn btn-blue btn-xs"><i class="fa fa-arrow-left" target="_blank"></i> Back To List</a>
                    <button ng-click="@refreshPage" class="btn btn-info btn-xs"><i class="fa fa-refresh"></i> Refresh</button>
                </div>
            </div><!--Widget Header end-->
            <div class="widget-body">
                <div class="row">
                  <div class="col-md-6 table-form">
                     <table class="form" width="100%" border="0" cellspacing="2" cellpadding="3">
                        <tbody>
                             <tr>
                                <td class="fieldlabel">Program Id </td>
                                <td class="fieldarea">
                                    <input type="number" ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_ProgramId" class="form-control" placeholder="Enter Program Id"   />
                                </td>
                                <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Program Id." data-toggle="tooltip"></i></span></td>
                            </tr>
                             <tr>
                                <td class="fieldlabel">Internal Bank Id </td>
                                <td class="fieldarea">
                                    <input type="number" ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_InternalBankId" class="form-control" placeholder="Enter Internal Bank Id"   />
                                </td>
                                <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Internal Bank Id." data-toggle="tooltip"></i></span></td>
                            </tr>
                             <tr>
                                <td class="fieldlabel">Voucher Prefix </td>
                                <td class="fieldarea">
                                    <input type="text" ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_VoucherPrefix" class="form-control" placeholder="Enter Voucher Prefix" maxlength="50"  />
                                </td>
                                <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Voucher Prefix." data-toggle="tooltip"></i></span></td>
                            </tr>
                            <tr>
                                <td class="fieldlabel">Remark </td>
                                <td class="fieldarea">
                                    <textarea type="text" ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_Remark" class="form-control" placeholder="Enter Remark"   />
                                    </textarea> 
                                </td>
                                <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter Remark if you need." data-toggle="tooltip"></i></span></td>
                            </tr>
                        
                             <tr>
                                <td class="fieldlabel">Is Enable </td>
                                <td class="fieldarea">
                                    <label class="checkbox-inline">
                                    <input type="checkbox" ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_IsEnable"  />
                                    </label>
                                </td>
                                <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Check Is Enable if required." data-toggle="tooltip"></i></span></td>
                             </tr>
                        
                             <tr>
                                <td class="fieldlabel">History </td>
                                <td class="fieldarea">
                                    <input type="text" ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_History" class="form-control" placeholder="Enter History"   />
                                </td>
                                <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Enter History." data-toggle="tooltip"></i></span></td>
                            </tr>
                             <tr>
                                <td class="fieldlabel">Is Active </td>
                                <td class="fieldarea">
                                    <label class="checkbox-inline">
                                    <input type="checkbox" ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_IsActive"  />
                                    </label>
                                </td>
                                <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Check Is Active if required." data-toggle="tooltip"></i></span></td>
                             </tr>
                        
                             <tr>
                                <td class="fieldlabel">Is Deleted </td>
                                <td class="fieldarea">
                                    <label class="checkbox-inline">
                                    <input type="checkbox" ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_IsDeleted"  />
                                    </label>
                                </td>
                                <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Check Is Deleted if required." data-toggle="tooltip"></i></span></td>
                             </tr>
                        
                   
                         <tr>
                            <td class="fieldlabel">Payment Gateway </td>
                            <td class="fieldarea">
                                <select ng-model="PaymentGatewayProgramMap.@Acnt_PaymentGatewayProgramMap.Property_GatewayId"
                                        ng-options="option.Id as option.Name for option in PaymentGatewayList " ng-init="" class="form-control select-inline">
                                    <option value="" label="" class="">---Select One---</option>
                                </select>
                            </td>
                            <td class="fieldarea-tooltip"><span><i class="fa fa-question-circle" data-title="Select Payment Gateway." data-toggle="tooltip"></i></span></td>
                        </tr>
                  

                            </tbody>
                        </table>
                    </div><!--Col end-->
                </div><!--Row end-->
            </div><!--Widget Body end-->
            <div class="widget-footer text-center">
                <div class="row">
                    <div class="col-md-6 text-center">
                        <button ng-click="@(savePaymentGatewayProgramMap)()" class="btn btn-md btn-success"><span class="fa fa-save"></span> Save</button>
                     </div>
                </div>
            </div>
        </div>
      </div><!--view permission end-->
    </div>
</div><!--controller end-->



