@using EMS.CoreLibrary
@using EMS.DataAccess.Data
@using EMS.Framework.Settings
@using EMS.Framework.Utils
@{
    ViewBag.Title = "Edit Profile";
    var StudentId = HttpUtil.Profile.UserId;

    //WebApi link for single row
    var getStudentByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "ProfileApi", action = "GetStudentById", area = "Student" });
    var getStudentDataExtraUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "ProfileApi", action = "GetStudentDataExtra", area = "Student" });
    var saveStudentUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "ProfileApi", action = "SaveStudent", area = "Academic" });
    var deleteStudentByIdUrl = Url.HttpRouteUrl("DefaultApi", new { controller = "ProfileApi", action = "GetDeleteStudentById", area = "Student" });
}

@section ScriptSection
{
    <script src="~/Areas/Student/Views/Profile/Edit.js?v=3"></script>
}
@section StyleSection
{
    <style>
        .profile-container .header-avatar {
            width: 160px;
            height: 160px;
            -webkit-border-radius: 50%;
            -webkit-background-clip: padding-box;
            -moz-border-radius: 50%;
            -moz-background-clip: padding;
            border-radius: 50%;
            background-clip: padding-box;
            border: 5px solid #ffffff;
            -webkit-box-shadow: 0 0 20px rgba(0, 84, 255, 0.35);
            -moz-box-shadow: 0 0 20px rgba(0, 84, 255, 0.35);
            box-shadow: 0 0 20px rgba(0, 84, 255, 0.35);
            margin: 25px auto;
            margin-left: 16px;
        }
    </style>
}
<div ng-controller="ProfileEditCtrl" ng-init="Init(
      '@StudentId'
