@{
    Layout = "~/Views/Shared/_LayoutBootstrap.cshtml";
}
@section CheckBrowserSection
{
    @RenderSection("CheckBrowserSection", required: false)
}
@section HeadSection
{
    @RenderSection("HeadSection", required: false)
}
@section StyleSection
{
    <!--css/google-fonts-->
    @*<link href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,600,700,300" rel="stylesheet" type="text/css">*@
    <link href="~/assets/fonts/google/fonts.googleapis.com.css" rel="stylesheet" />
    <!--css/theme-->
    <link id="theme" href="@Styles.Url("~/css/theme")" rel="stylesheet" type="text/css" />
    @*@Styles.Render("~/css/theme-rtl")*@
    <link id="skin-link" href="" rel="stylesheet" type="text/css" />
    @RenderSection("StyleSection", required: false)
}
@RenderBody()
@section ScriptSection
{
    <!--js/theme-->
    @Scripts.Render("~/js/theme")
    @RenderSection("ScriptSection", required: false)
}

