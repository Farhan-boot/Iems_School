    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewBag.Title | @SiteSettings.Instance.InstituteShortName - @SiteSettings.Instance.ProductFullName - @SiteSettings.PoweredByFullText</title>
    <meta name="description" content="@ViewBag.Description | @SiteSettings.Instance.ProductFullName, @SiteSettings.PoweredByFullText" />
